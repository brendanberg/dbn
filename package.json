        "mocha": "^10.2.0",
        "pegjs": "^0.10.0",
        "pegjs-loader": "github:brendanberg/pegjs-loader",
        "webpack": "^5.93.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^5.2.1"
    },
    "dependencies": {
        "axios": ">=0.21.1"
    },
    "license": "MIT"
}
