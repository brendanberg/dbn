{
    "name": "draw_by_numeral",
    "version": "0.1.0",
    "author": "Brendan Berg <brendan@berg.industries>",
    "description": "A recreation of John Maeda's Design by Numbers environment",
    "contributors": [
        {
            "name": "Brendan Berg",
            "email": "brendan@berg.industries"
        }
    ],
    "scripts": {
        "clean": "rm -rf build/* && rm -rf node_modules/",
